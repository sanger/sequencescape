<br>
<% if sample_manifest.asset_type == "1dtube" %>
  <%= form_for :pooling, url: new_pooling_path do |form| -%>
    <% sample_manifest.barcodes.each do |barcode| %>
      <%= form.hidden_field :barcodes, multiple: true, value: barcode %>
    <% end %>
    <%= form.submit 'Pool' %>
  <% end %>
<% end %>