<h1>Edit robot property for <%= @robot.name %></h1>

<% form_for([@robot, @robot_property]) do |f| %>
<%= f.error_messages %>
<p>
<%= f.label :name %>
<br />
<%= f.text_field :name %>
</p>
<p>
<%= f.label :value %>
<br />
<%= f.text_field :value %>
</p>
<p>
<%= f.label :key %>
<br />
<%= f.text_field :key %>
</p>
<p>
<%= f.submit "Update" %>
</p>
<% end %>
<%= link_to 'Show', robot_robot_property_path(@robot,@robot_property) %> |
<%= link_to 'Back', robot_robot_properties_path(@robot) %>
