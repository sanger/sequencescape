
<%= form_for(location_report_form, html: {id: "new_report_from_labwhere_location"}) do |f| %>
  <div class='form-group'>
    <%= f.label 'name', 'Report name' %><br>
    <%= f.text_field :name, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :location_barcode, 'LabWhere location barcode' %>
    <%= f.text_field :location_barcode, class: 'form-control' %>
  </div>

  <%= f.hidden_field :report_type, value: 'type_labwhere' %>

  <div class="form-group actions-group">
    <%= f.submit('Create report from labwhere', autocomplete: 'off',class: 'btn btn-success') %>
  </div>
<% end %>
