<h1>New asset group</h1>

<%= form_for @asset_group,  url: study_asset_groups_path(@study) do |f| %>
  <%= f.error_messages %>

  <p><%= f.label(:name, 'Name') %>: <%= f.text_field :name %></p>
  <p><%= f.submit "Create" %></p>
<% end %>

<%= link_to 'Back', study_asset_groups_path(@study) %>
