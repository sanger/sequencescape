<div class="page-header"><h1>Editing Program</h1></div>

<%= form_for([:admin,@program]) do |f| %>
  <%= render_error_messages(@program) %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', [:admin,@program] %> |
<%= link_to 'Back', admin_programs_path %>
