<h1>New robot property for <%= @robot.name %></h1>

<%= form_for([:admin,@robot, @robot_property]) do |f| %>
<%= f.error_messages %>
<p>
<%= f.label :name %>
<br />
<%= f.text_field :name %>
</p>
<p>
<%= f.label :value %>
<br />
<%= f.text_field :value %>
</p>
<p>
<%= f.label :key %>
<br />
<%= f.text_field :key %>
</p>
<p>
<%= f.submit "Create" %>
</p>
<% end %>
<%= link_to 'Back', admin_robot_robot_properties_path(@robot) %>
