
<div class="page-header"><h1>Editing Plate Purpose</h1></div>

<%= form_for([:admin,@plate_purpose]) do |f| %>
  <%= render_error_messages(@plate_purpose) %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', [:admin, @plate_purpose] %> |
<%= link_to 'Back', admin_plate_purposes_path %>
