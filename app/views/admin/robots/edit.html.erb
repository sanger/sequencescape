
<div class="page-header"><h1>Editing robot</h1></div>

<%= form_for(@robot, url: admin_robot_path(@robot)) do |f| %>
  <%= render_error_messages(@robot) %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :location %><br />
    <%= f.text_field :location %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', admin_robot_path(@robot) %> |
<%= link_to 'Back', admin_robots_path %>
